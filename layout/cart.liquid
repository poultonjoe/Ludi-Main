<script>$(document).ready(function(){
            // Move the logo up by 50% after 1 second
            setTimeout(function() {
                $('.nav-logo-container').css('transform', 'translateY(-50%)');
            }, 1000);
            // Check scroll depth and reverse the position if scrolled more than 0px
            $(window).scroll(function() {
                if ($(this).scrollTop() > 0) {
                    $('.nav-logo-container').css('transform', 'translateY(0%)');
                    $('.navbar-screen').css({
                        'backdrop-filter': 'blur(0.25rem)',
                        'border-bottom-color': 'rgba(239, 236, 228, 1)'
                    });
                } else {
                    $('.nav-logo-container').css('transform', 'translateY(-50%)');
                    $('.navbar-screen').css({
                        'backdrop-filter': 'blur(0rem)',
                        'border-bottom-color': 'rgba(239, 236, 228, 0)'
                    });
                }
            });
        });
    </script><script>$(document).ready(function() {
    var $menuBar = $('.nav_component'); // Adjust this selector to your menu bar
    var $whiteDiv = $('.scroll-trigger'); // Adjust this selector to your white div
    var whiteDivOffset = $whiteDiv.offset().top; // Get the offset of the white div from the top
    $(window).scroll(function() {
        var scrollPosition = $(window).scrollTop(); // Get the current scroll position
        if (scrollPosition >= whiteDivOffset) {
            $menuBar.css('background-color', '#034732'); // Change the menu background color when above the white div
        } else {
            $menuBar.css('background-color', 'transparent'); // Reset to original color (or any other color)
        }
    });
});
</script>
